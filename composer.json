{
    "name": "phillarmonic/syncopate-bundle",
    "description": "Symfony bundle for integrating with SyncopateDB",
    "type": "symfony-bundle",
    "version": "1.2.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Phillarmonic Software Team",
            "email": "phillarmonicsoftware@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/cache": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/finder": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Phillarmonic\\SyncopateBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Philharmonic\\SyncopateBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|10.0|11.0",
        "symfony/phpunit-bridge": "^6.0|^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
